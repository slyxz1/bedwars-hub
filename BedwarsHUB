local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Sly's Bedwars Hub", "Midnight")
local Tab = Window:NewTab("Main")
local Section = Tab:NewSection("SCRIPTS")
Section:NewButton("Vape V4", "MOST USED BEDWARS SCRIPT", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
    print("Clicked")
end)
Section:NewButton("Inf Jump", "Infinite Jump", function()
    local InfiniteJumpEnabled = true
game:GetService("UserInputService").JumpRequest:connect(function()
	if InfiniteJumpEnabled then
		game:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
	end
end)
    print("Clicked")
end)
Section:NewButton("Rise", "Rise Script", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/RiseForRoblox/main/main.lua", true))()
    print("Clicked")
end)
local Section = Tab:NewSection("MOBILE SCRIPTS")
Section:NewButton("Vezt", "VeztWare", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/VeztWare/Vezt/main/Vezt%20-%20Bedwars.lua"))()
    print("Clicked")
end)
Section:NewButton("Aurora X", "Aurora", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/XStormDeveloper/AuroraForRoblox/main/lua", true))()
    print("Clicked")
end)
